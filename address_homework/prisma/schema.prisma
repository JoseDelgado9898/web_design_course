// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model address {
  id          Int      @id @default(autoincrement())
  name        String
  homePhone   String @db.VarChar(8)
  workPhone   String @db.VarChar(8)
  homeAddress String @db.VarChar(200)
  workAddress String @db.VarChar(200)
}
